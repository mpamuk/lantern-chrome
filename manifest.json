{
  "name": "Lantern Chrome Extension",
  "version": "1.0",
  "description": "This plugin is showing how Facebook Connect/Graph API works on Chrome Extension.",
  "background":
  {"page":"background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon":"lantern3.png"
  },
  "manifest_version":2,
  "permissions": [
    "tabs",
    "http://*.facebook.com/*",
    "http://whitelist-api.herokuapp.com/*",
    "cookies"
  ]
}
